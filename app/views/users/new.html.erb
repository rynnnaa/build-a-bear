<h1> Sign Up </h1>
<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :password_digest %>
  <%= f.text_field :password_digest %><br>
  <%= f.label :age %>
  <%= f.text_field :age %><br>
  <br>
  <%= f.submit %>
<% end %>